<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">

            <solid android:color="@color/calendar_grey_background_stroke_color"/>
            <corners android:radius="5dp"/>
            <stroke android:color="@color/calendar_grey_background_stroke_color" android:width="1dp"
                ></stroke>


        </shape>
    </item>
    <item android:top="2dp" android:bottom="2dp" android:left="2dp" android:right="2dp">
    <bitmap xmlns:android="http://schemas.android.com/apk/res/android"
                android:src="@drawable/grey_line"
                android:tileMode="repeat"
                android:antialias="true"/>
    </item>
</layer-list>